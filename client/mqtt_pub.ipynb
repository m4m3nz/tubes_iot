    "import paho.mqtt.client as mqtt\n",
    "client = mqtt.Client()\n",
    "client.connect(\"teknik-fisika.or.id\" ,1883, 60)\n",
    "\n",
    "while True:\n",
    "    payload = input(\"Pesan :\")\n",
    "    client.publish(\"IoT_Command\", payload)\n",
    "\n",
    "client.loop_forever()"
